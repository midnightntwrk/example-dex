{
  "name": "example-dex",
  "version": "1.0.0",
  "description": "A simple AMM DEX template working on Midnight",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "test": "rm -rf contract/managed && compact compile --skip-zk contract/src/contract.compact contract/managed && vitest",
    "compile-contract": "compact compile contract/src/contract.compact contract/managed",
    "verify-contract": "compact compile --skip-zk contract/src/contract.compact contract/no-zk",
    "watch-compile": "nodemon --exec \"npm run verify-contract\" --ext compact",
    "hash-oz-fungible-token-contract": "node tests/utils.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/midnightntwrk/example-dex.git"
  },
  "keywords": [
    "midnight",
    "compact",
    "dex"
  ],
  "author": "Claude Barde",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/midnightntwrk/example-dex/issues"
  },
  "homepage": "https://github.com/midnightntwrk/example-dex#readme",
  "devDependencies": {
    "@midnight-ntwrk/midnight-js-testing": "^2.0.2",
    "axios": "^1.11.0",
    "nodemon": "^3.1.10",
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1",
    "testcontainers": "^11.5.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "^0.8.1",
    "@midnight-ntwrk/ledger": "^4.0.0",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "^2.0.2",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "^2.0.2",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "^2.0.2",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "^2.0.2",
    "@midnight-ntwrk/midnight-js-utils": "^2.0.2",
    "@midnight-ntwrk/wallet": "^5.0.0",
    "@midnight-ntwrk/wallet-sdk-hd": "^2.0.0",
    "@midnight-ntwrk/zswap": "^4.0.0"
  }
}
